<h3>Add a new Task</h3>
<%= form_for :list, url: lists_path do |f| %>
    <p>
        <%= f.label :Title %><br>
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.label :Notes %><br>
        <%= f.text_area :nitle %>
    </p>
    <p>
        <%= f.label :Dues %><br>
        <%= f.date_field :due %>
    </p>
    <%= radio_button_tag(:priority, "high") %>
    <%= label_tag(:priority, "High") %>
    <%= radio_button_tag(:priority, "medium") %>
    <%= label_tag(:priority, "Medium") %>
    <%= radio_button_tag(:priority, "low") %>
    <%= label_tag(:priority, "low") %>

    <p>
     <%= f.submit %>
    </p>
<% end %>