<h3>Add a new Task</h3>
<%= form_for :list, url: lists_path do |f| %>
    <p>
        <%= f.label :Title %><br>
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.label :Notes %><br>
        <%= f.text_area :note %>
    </p>
    <p>
        <%= f.label :Dues %><br>
        <%= f.date_field :due %>
    </p>
    <p>
        <%= f.label :priority %><br>
        <%= f.text_field :priority %>
    </p>
    

    <p>
     <%= f.submit %>
    </p>
<% end %>